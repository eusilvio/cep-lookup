{
  "name": "@eusilvio/cep-lookup",
  "version": "2.4.0",
  "description": "A agnostic, performant and flexible CEP lookup library with race strategy and caching.",
  "license": "MIT",
  "author": "Silvio Souza",
  "repository": {
    "type": "git",
    "url": "https://github.com/eusilvio/cep-lookup.git",
    "directory": "packages/cep-lookup"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./providers": {
      "types": "./dist/src/providers/index.d.ts",
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.cjs"
    },
    "./providers/viacep": {
      "types": "./dist/src/providers/viacep.d.ts",
      "import": "./dist/providers/viacep.mjs",
      "require": "./dist/providers/viacep.cjs"
    },
    "./providers/brasil-api": {
      "types": "./dist/src/providers/brasil-api.d.ts",
      "import": "./dist/providers/brasil-api.mjs",
      "require": "./dist/providers/brasil-api.cjs"
    },
    "./providers/apicep": {
      "types": "./dist/src/providers/apicep.d.ts",
      "import": "./dist/providers/apicep.mjs",
      "require": "./dist/providers/apicep.cjs"
    },
    "./providers/opencep": {
      "types": "./dist/src/providers/opencep.d.ts",
      "import": "./dist/providers/opencep.mjs",
      "require": "./dist/providers/opencep.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc --emitDeclarationOnly --outDir dist && esbuild src/index.ts src/providers/index.ts src/providers/viacep.ts src/providers/brasil-api.ts src/providers/apicep.ts src/providers/opencep.ts --bundle --platform=neutral --format=cjs --outdir=dist --out-extension:.js=.cjs --minify && esbuild src/index.ts src/providers/index.ts src/providers/viacep.ts src/providers/brasil-api.ts src/providers/apicep.ts src/providers/opencep.ts --bundle --platform=neutral --format=esm --outdir=dist --out-extension:.js=.mjs --minify",
    "test": "jest",
    "test:html": "npx http-server . -o examples/index.html -c-1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "esbuild": "^0.27.2",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
