{
  "name": "@eusilvio/cep-lookup-vue",
  "version": "0.3.0",
  "description": "Vue 3 composition API hooks for @eusilvio/cep-lookup",
  "license": "MIT",
  "author": "Silvio Souza",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc --build && esbuild src/index.ts --bundle --platform=neutral --format=cjs --outdir=dist --out-extension:.js=.cjs --minify --external:vue --external:@eusilvio/cep-lookup && esbuild src/index.ts --bundle --platform=neutral --format=esm --outdir=dist --out-extension:.js=.mjs --minify --external:vue --external:@eusilvio/cep-lookup",
    "test": "jest"
  },
  "peerDependencies": {
    "@eusilvio/cep-lookup": "^2.3.2",
    "vue": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eusilvio/cep-lookup.git",
    "directory": "packages/cep-lookup-vue"
  },
  "bugs": {
    "url": "https://github.com/eusilvio/cep-lookup/issues"
  },
  "homepage": "https://github.com/eusilvio/cep-lookup#readme",
  "keywords": [
    "cep",
    "lookup",
    "vue",
    "hook",
    "composition-api"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@vue/test-utils": "^2.4.6",
    "esbuild": "^0.27.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "vue": "^3.4.0"
  }
}
