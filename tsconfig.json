{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "lib": ["ES2021", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@eusilvio/cep-lookup": ["packages/cep-lookup/src"],
      "@eusilvio/cep-lookup/*": ["packages/cep-lookup/src/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}